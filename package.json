{
  "name": "@balancer/frontend-v3",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "balancer/frontend-v3",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:all": "pnpm run lint && pnpm run stylelint && pnpm run prettier && pnpm run typecheck",
    "stylelint": "stylelint '**/*.css'",
    "stylelint:fix": "pnpm run stylelint -- --fix",
    "prettier": "prettier --check '**/*.*(md|json|yaml|ts|js|tsx)' --loglevel warn",
    "prettier:fix": "pnpm run prettier --write",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prepare": "husky install",
    "gen:graphql": "DOTENV_CONFIG_PATH=./.env.local graphql-codegen -r dotenv/config",
    "gen:theme-typings": "chakra-cli tokens ./lib/services/chakra/theme.ts",
    "postinstall": "npm run gen:theme-typings",
    "test": "pnpm test:unit & pnpm run test:integration",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest --coverage",
    "test:unit:silent:coverage": "SILENT_TESTS=true vitest --coverage",
    "test:integration": "vitest -c ./vitest.config.integration.ts"
  },
  "dependencies": {
    "@apollo/client": "3.8.0-rc.1",
    "@apollo/experimental-nextjs-app-support": "^0.4.1",
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@rainbow-me/rainbowkit": "^1.0.6",
    "@tanstack/react-table": "^8.9.3",
    "@types/node": "20.3.2",
    "@types/react-dom": "18.2.6",
    "@uidotdev/usehooks": "^2.0.1",
    "deepmerge": "^4.3.1",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "framer-motion": "^10.13.0",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "next": "13.4.9",
    "numeral": "^2.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1",
    "typescript": "5.1.6",
    "viem": "^1.2.9",
    "wagmi": "^1.3.10",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@chakra-ui/styled-system": "^2.9.1",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/client-preset": "^4.0.1",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript-document-nodes": "^4.0.1",
    "@graphql-codegen/typescript-msw": "^1.1.6",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@tanstack/react-query": "^4.29.19",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/lodash": "^4.14.195",
    "@types/numeral": "^2.0.2",
    "@types/react": "18.2.14",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@viem/anvil": "^0.0.6",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.4",
    "autoprefixer": "^10.4.14",
    "cross-fetch": "^4.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "happy-dom": "^10.5.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "lokijs": "^1.5.12",
    "msw": "^1.2.3",
    "prettier": "^2.8.8",
    "stylelint": "^15.9.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0",
    "vitest": "^0.34.4",
    "vitest-mock-extended": "^1.2.1"
  },
  "pnpm": {
    "overrides": {
      "@tanstack/react-query": "^4.29.19"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "@types/react": "^18.0.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
      }
    }
  }
}
