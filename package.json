{
  "name": "@balancer/frontend-v3",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "balancer/frontend-v3",
  "private": true,
  "scripts": {
    "dev": "PROTOCOL=bal next dev",
    "dev-beets": "PROTOCOL=beets next dev",
    "build": "PROTOCOL=bal next build",
    "build-beets": "PROTOCOL=beets next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix && pnpm run prettier:fix",
    "lint:all": "pnpm run lint && pnpm run stylelint && pnpm run prettier && pnpm run typecheck",
    "stylelint": "stylelint '**/*.css'",
    "stylelint:fix": "pnpm run stylelint -- --fix",
    "prettier": "prettier --check '**/*.*(md|json|yaml|ts|js|tsx)' --loglevel warn",
    "prettier:fix": "pnpm run prettier --write",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prepare": "husky install",
    "gen:graphql": "DOTENV_CONFIG_PATH=./.env.local graphql-codegen -r dotenv/config",
    "gen:theme-typings": "chakra-cli tokens ./lib/shared/services/chakra/theme.ts",
    "gen:wagmi": "pnpm wagmi generate",
    "test": "pnpm test:unit & pnpm run test:integration",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest --coverage",
    "test:unit:silent:coverage": "SILENT_TESTS=true vitest --coverage",
    "test:integration": "vitest -c ./vitest.config.integration.ts",
    "postinstall": "npm run gen:theme-typings"
  },
  "dependencies": {
    "@apollo/client": "3.8.0-rc.1",
    "@balancer/sdk": "github:balancer/b-sdk#price-impact-with-dist",
    "@chakra-ui/anatomy": "^2.2.2",
    "@chakra-ui/hooks": "^2.2.1",
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "^2.8.1",
    "@chakra-ui/theme-tools": "^2.1.1",
    "@dicebear/collection": "^7.0.1",
    "@dicebear/core": "^7.0.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@nikolovlazar/chakra-ui-prose": "^1.2.1",
    "@rainbow-me/rainbowkit": "^1.1.1",
    "@tanstack/react-query": "^4.29.19",
    "@tanstack/react-table": "^8.9.3",
    "@types/echarts": "^4.9.19",
    "@types/node": "20.3.2",
    "@types/react-dom": "18.2.6",
    "@uidotdev/usehooks": "^2.0.1",
    "@vercel/speed-insights": "^1.0.1",
    "bignumber.js": "^9.1.2",
    "date-fns": "^2.30.0",
    "deepmerge": "^4.3.1",
    "echarts": "^5.4.3",
    "echarts-for-react": "^3.0.2",
    "eslint": "8.43.0",
    "eslint-config-next": "14.0.1",
    "framer-motion": "^10.13.0",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "next": "14.0.4",
    "next-usequerystate": "^1.9.2",
    "nextjs-toploader": "^1.6.4",
    "numeral": "^2.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hotkeys-hook": "^4.4.1",
    "react-icons": "^4.10.1",
    "react-jazzicon": "^1.0.4",
    "react-tiny-virtual-list": "^2.2.0",
    "tinycolor2": "^1.6.0",
    "typescript": "5.1.6",
    "use-debounce": "^10.0.0",
    "viem": "^1.18.1",
    "wagmi": "^1.4.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@apollo/experimental-nextjs-app-support": "^0.5.1",
    "@chakra-ui/cli": "^2.4.1",
    "@chakra-ui/styled-system": "^2.9.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript-document-nodes": "^4.0.1",
    "@graphql-codegen/typescript-msw": "^1.1.6",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.14.195",
    "@types/numeral": "^2.0.2",
    "@types/react": "18.2.34",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@viem/anvil": "^0.0.6",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.1",
    "@wagmi/cli": "^1.5.1",
    "autoprefixer": "^10.4.14",
    "c8": "^8.0.1",
    "cross-fetch": "^4.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "lokijs": "^1.5.12",
    "msw": "2.0.10",
    "prettier": "^2.8.8",
    "stylelint": "^15.9.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0",
    "vitest": "^1.0.4",
    "vitest-mock-extended": "^1.3.1"
  },
  "pnpm": {
    "overrides": {
      "@tanstack/react-query": "^4.29.19"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "@types/react": "^18.0.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
      }
    }
  },
  "engineStrict": true,
  "engines": {
    "node": ">=18.x"
  }
}
